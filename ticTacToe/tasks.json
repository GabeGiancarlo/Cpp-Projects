{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build ticTacToe",
      "type": "shell",
      "command": "/usr/bin/clang++",
      "args": [
        "-std=c++14",
        "-stdlib=libc++",
        "-g",
        "${workspaceFolder}/main.cpp",
        "-o",
        "${workspaceFolder}/main"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
